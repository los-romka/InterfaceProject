Истории болезней #TABLE# { 
  ~set История болезни #ROW# {
      Пациент {
      Имя [str]
      Возраст [str]
    }
    Жалоба {
       Описание [str]
       Важность ~ALT {
           ["Очень важно"]
           ["Важно"]
           ["Средней важности"]
           ["Неважно"]
       }
    }
    Динамика #TABLE# { 
      ~set Событие ~proxy #ROW# {
        Время [date]
        Описание события [str]
      }
    }    
  }
}